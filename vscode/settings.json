{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "autoimport.autoComplete": false,
  "autoimport.useSemiColon": false,
  "breadcrumbs.symbolPath": "last",
  "debug.console.fontFamily": "'MonoLisa Variable', monospace",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.maxComputationTime": 0,
  "editor.accessibilitySupport": "off",
  "editor.renderWhitespace": "boundary",
  "editor.autoClosingBrackets": "languageDefined",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 60,
  "editor.autoIndent": "advanced",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.codeLensFontFamily": "editor",
  "editor.codeLensFontSize": 12,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.experimentalEditContextEnabled": true,
  "editor.fontFamily": "'MonoLisa Variable', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.fontVariations": false,
  "editor.fontWeight": "normal",
  "editor.guides.bracketPairs": "active",
  "editor.inlayHints.fontSize": 12,
  "editor.inlayHints.padding": true,
  "editor.inlayHints.maximumLength": 43,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.minimap.size": "fill",
  "editor.minimap.scale": 2,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 120,
  "editor.pasteAs.preferences": ["text.updateImports"],
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.trimAutoWhitespace": true,
  "editor.lineNumbers": "relative",
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.tsx": "typescriptreact",
    "*.css": "tailwindcss",
    "*.tf": "terraform",
    "*.ts": "typescript"
  },
  "files.exclude": {
    "**/node_modules/**": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": true
  },
  "githubPullRequests.createOnPublishBranch": "never",
  "githubPullRequests.pullBranch": "never",
  "githubPullRequests.terminalLinksHandler": "github",
  "go.toolsManagement.autoUpdate": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.preview.fontFamily": "",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "prettier.singleQuote": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingImports": "none",
    "reportMissingModuleSource": "none"
  },
  "redhat.telemetry.enabled": false,
  "scm.inputFontFamily": "'JetBrains Mono Nerd Font', MonoLisa, Menlo, Monaco, 'Courier New', monospace",
  "search.exclude": {
    "**/node_modules/**": true
  },
  "search.useGlobalIgnoreFiles": true,
  "search.defaultViewMode": "tree",
  "search.mode": "reuseEditor",
  "search.searchOnTypeDebouncePeriod": 1000,
  "search.searchEditor.defaultNumberOfContextLines": 2,
  "search.searchEditor.focusResultsOnSearch": true,
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "terminal.explorerKind": "external",
  "terminal.integrated.altClickMovesCursor": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "'MonoLisa Variable', monospace",
  "typescript.experimental.expandableHover": true,
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.nextInFiles"],
      "silent": true
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prevInFiles"],
      "silent": true
    }
  ],
  "window.restoreWindows": "one",
  "workbench.colorTheme": "Catppuccin Frapp√©",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.fontAliasing": "auto",
  "workbench.iconTheme": "vscode-great-icons",
  "workbench.list.smoothScrolling": true,
  "workbench.localHistory.exclude": {
    "**/cache/**": true,
    "**/node_modules/**": true,
    "**/transpiled/**": true
  },
  "workbench.sideBar.location": "right",
  "xml.server.vmargs": "-Xmx128M",
  "zenMode.fullScreen": false,
  "scm.diffDecorations": "gutter",
  "cursor.cpp.disabledLanguages": ["yaml", "plaintext"]
}

